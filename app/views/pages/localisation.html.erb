<% resource_cache(resource) do %>
  <article class="<%= resource_instance_name %>">
    <%= render "google_map" %>
    <div class="container">
      <div class="row">
        <div class="col-lg-12 article-body">
          <%= render_page_title(resource.title) if resource.show_title_in_page %>
          <%= render_copy_text(resource.body) %>
        </div>
      </div>
    </div>
	</article>
<% end %>